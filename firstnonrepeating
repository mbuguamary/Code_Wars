 let str = 'maaary'
 const nonRepeating = (str) => {
    let obj = {}
    for (let item of str){
        if(obj[item]){
        obj[item]++;

    }else{
        obj[item]=1;
    }
    }
    console.log(obj)
   for(let item in obj){
    if(obj[item===1]){
        return item
    //return str.indexOf(item);
    }
   }
 }
console.log(nonRepeating(str))